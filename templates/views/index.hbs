function changeImg(){ 
    document.whole.src = images[i]; 
          
    if(i < images.length - 1){ //Image.length is 5
               
        i++; 
    } 
    else {  
        i = 0;
    }
        
    setTimeout("changeImg()", time); 
    }
    window.onload=changeImg;

